{
  "features": {
    "copilot": false
  },
  "theme": "Solarized Light",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "vim_mode": true,
  "cursor_blink": false,
  "inlay_hints": {
    "enabled": true
  },
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "file_scan_exclusions": [".node_modules/", ".git"],
  "scrollbar": {
    "show": "never"
  }
}
